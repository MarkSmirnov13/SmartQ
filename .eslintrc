{
  "extends": [
    "airbnb-base",
    "prettier"
  ],
  "plugins": [
    "babel",
    "prettier"
  ],
  "env": {
    "node": true
  },
  "parser": "babel-eslint",
  "rules": {
    "semi": 0,
    "no-console": 0,
    "import/prefer-default-export": "off",
    "prettier/prettier": [
      "error", {
        "semi": false,
        "printWidth": 100,
        "tabWidth": 2
      }
    ]
  }
}
